{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "Suicide and GDP per capita ",
  "width": 800,
  "height": 450,
  "data": {"url": "https://raw.githubusercontent.com/IshanJeetun/FIT3179/main/master.csv"},

 "transform": [
    {
      "joinaggregate": [{"op": "mean","field": "gdp_per_capita ($)","as": "mean1"}],
      "groupby": ["country"]
    },
    {
      "joinaggregate": [{"op": "mean","field": "suicides/100k pop","as": "mean2"}],
      "groupby": ["country"]
    }
  ],
  "mark": "circle",
  "encoding": {
    "x": {"field": "mean1", "type": "quantitative",
      "title": "GDP per capita ($)"},
    "y": {"field": "mean2", "type": "quantitative", "title": "Average Suicides per 100k pop"},
    "color": {"field": "country", "type": "nominal"},
    "tooltip": [
      {"field": "mean1", "type": "quantitative"},
      {"field": "mean2", "type": "quantitative"},
      {"field": "country", "type": "nominal"}
    ]
  }
}
