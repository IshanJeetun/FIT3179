{
  "data": { "url": "https://raw.githubusercontent.com/IshanJeetun/FIT3179/main/master.csv"},
  "title": "Suicide number by age group",
  "height": {"step": 30},
  "width": 400,
  "transform": [{"filter": "datum.year == 2000"}],
  "mark": "bar",
  "encoding": {
    "y": {"field": "age", "title": "Age group",
        "sort": ["75+ years", "55-74 years", "35-54 years", "25-34 years", "15-24 years", "5-14 years"]},
    "x": {
      "aggregate": "sum", "field": "suicides_no",
      "title": "Total number of suicides"
    }
  }
}
